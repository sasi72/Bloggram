<%- include("partials/header.ejs") %>
<div class="form-signin w-50 mt-50 container">
  <form>
    <h1 class="h3 mb-3 fw-normal">Spread your Innovations!!!</h1>

    <div class="form-floating my-3">
      <input type="name" class="form-control" id="floatingInput" placeholder="Your Name">
      <label for="floatingInput">Your Name</label>
    </div>

    <div class="form-floating my-3">
      <input type="password" class="form-control" id="floatingPassword" placeholder="Password">
      <label for="floatingPassword">Password</label>
    </div>

    <div class="form-check text-start my-3">
      <input class="form-check-input" type="checkbox" value="remember-me" id="flexCheckDefault">
      <label class="form-check-label" for="flexCheckDefault">
        Remember me
      </label>
    </div>

    <button class="btn btn-primary w-100 py-2" type="signIn">Sign in</button>

    <p class="mt-5 mb-3 text-body-secondary">NOTE : You can Add, Modify and Delete your Blog for only one Instance as Database is not embbeded yet.</p>
  </form>
</div>
<%- include("partials/footer.ejs") %>